{
    "name": "infinitypaul/laravel-database-filter",
    "description": "Need to filter database results with a query string? Here's a beautiful, easy to extend laravel package to keep your code super tidy.",
    "keywords": [
        "infinitypaul",
        "laravel-database-filter",
        "php filter",
        "model filter",
        "Open Source",
        "laravel 6",
        "laravel 7",
        "laravel 8",
        "github"
    ],
    "homepage": "https://github.com/infinitypaul/laravel-database-filter",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Paul Edward",
            "email": "infinitypaul@live.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.2",
        "illuminate/contracts": "^7.0|^8.0|^9.0|^10.0|^11.1|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.1.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Infinitypaul\\LaravelDatabaseFilter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Infinitypaul\\LaravelDatabaseFilter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Infinitypaul\\LaravelDatabaseFilter\\LaravelDatabaseFilterServiceProvider"
            ]
        }
    }
}
